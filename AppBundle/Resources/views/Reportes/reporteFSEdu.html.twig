                        <tr class="piede-tabla borde-arriba">
                            <td>Total</td>
                            <td class="celda">{{ tefexr }}</td>
                            <td></td>
                            <td class="celda">{{ tefexrh }}</td>
                            <td></td>
                            <td class="celda">{{ tefexrm }}</td>
                            <td></td>
                        </tr>
                    </tbody>    
                </table> 
            </td>
        </tr>
        
    </table>  
</page>                  
