                            {% set totalerh= totalerh + datoser.canth %}
                            {% set totalerm = totalerm + datoser.cantm %}
                        {% endfor %}
                        <tr class="piede-tabla borde-arriba">
                            <td>Total</td>
                            <td class="celda">{{ totalesr }}</td>
                            <td></td>
                            <td class="celda">{{ totalerh }}</td>
