{% extends '::basemenu.html.twig' %}

{% block header -%}
    Información Datos Demográficos - 
    <small>
        <i class="ace-icon fa fa-angle-double-right"></i>
        ver toda la información
    </small>
{% endblock %} 
   
{% block content -%}
    <h1>
        Id Encuesta: {{ boleta }}
    </h1> 
    <ul class="">
        <li>
            <a href="{{ path('datosdfamilia',{'idenc': boleta})  }}">
                Regresar al listado
            </a>
        </li>
    </ul>  
    <table class="table table-bordered table-striped table-responsive">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Departamento</th>
                <td>{{ entity.codDepartamento }}</td>
            </tr>
            <tr>
                <th>Municipio</th>
                <td>{{ entity.codMunicipio }} - {{ entity.nomMunicipio }}</td>
            </tr>
            <tr>
                <th>Comunidad</th>
                <td>{{ entity.codComunidad }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Sexo</th>
                <td>{% if entity.sexo == 1 %} Masculino {% else %} Femenido {% endif %}</td>
            </tr>
            <tr>
                <th>Edad</th>
                <td>{{ entity.edad }}</td>
            </tr>
            <tr>
                <th>Parentesco</th>
                <td>{{ entity.desParentesco }}</td>
            </tr>
            <tr>
                <th>Etnia</th>
                <td>{{ entity.desEtnia }}</td>
            </tr>
            <tr>
                <th>Tiene partida nacimiento?</th>
                <td>{% if entity.tienePn == 1 %}Si{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('datosdfamilia',{'idenc': boleta}) }}">
            Regresar a la lista
        </a>
    </li>
    <!--li>
        <a href="{{ path('datosdfamilia_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li-->
</ul>
{% endblock %}
